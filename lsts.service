[Unit]
Description=Local Speed Test Server
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/var/www/html/lsts/speedtest/main
ExecStart=/var/www/html/lsts/speedtest/main/bin/python3 /var/www/html/lsts/speedtest/main/app.py
RestartSec=5
Restart=always

[Install]
WantedBy=multi-user.target
